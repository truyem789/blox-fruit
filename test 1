-- Scripts:

local function UCTNDA_fake_script() -- Open.LocalScript 
	local script = Instance.new('LocalScript', Open)

	local frame = script.Parent.Parent.Frame
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = true
	end)
	
end
coroutine.wrap(UCTNDA_fake_script)()
local function NVPK_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
	
end
coroutine.wrap(NVPK_fake_script)()
local function RMAWHWN_fake_script() -- NightHub.LocalScript 
	local script = Instance.new('LocalScript', NightHub)

	loadstring(game:HttpGet("https://raw.githubusercontent.com/NightsTimeZ/Night_Script/main/BloxFruit.lua"))()
end
coroutine.wrap(RMAWHWN_fake_script)()
